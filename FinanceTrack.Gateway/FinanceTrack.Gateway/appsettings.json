{
  "ReverseProxy": {
    "Routes": {
      "spa": {
        "ClusterId": "spa-cluster",
        "Match": {
          "Path": "/{**catch-all}"
        }
      },
      "keycloak-realms": {
        "ClusterId": "keycloak-cluster",
        "Match": {
          "Path": "/realms/{**catch-all}"
        }
      },
      "keycloak-resources": {
        "ClusterId": "keycloak-cluster",
        "Match": {
          "Path": "/resources/{**catch-all}"
        }
      },
      "keycloak-admin": {
        "ClusterId": "keycloak-cluster",
        "Match": {
          "Path": "/admin/{**catch-all}"
        }
      }
      // add api routes here
    },
    "Clusters": {
      "spa-cluster": {
        "Destinations": {
          "spa": {
            "Address": "http://localhost:20000/"
          }
        }
      },
      "keycloak-cluster": {
        "Destinations": {
          "keycloak": {
            "Address": "http://localhost:16000/"
          }
        }
      }
    }
  }
}
